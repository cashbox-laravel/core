    "type": "library",
    "keywords": [
        "bank",
        "billing",
        "cashier",
        "payments"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@dragon-code.pro",
            "homepage": "https://github.com/andrey-helldar"
        }
    ],
    "support": {
        "issues": "https://github.com/cashier-provider/foundation/issues",
        "source": "https://github.com/cashier-provider/foundation"
    },
    "funding": [
