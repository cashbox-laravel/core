        },
        {
            "type": "boosty",
            "url": "https://boosty.to/dragon-code"
        },
        {
            "type": "yoomoney",
            "url": "https://yoomoney.ru/to/410012608840929"
        }
    ],
    "require": {
        "php": "^8.1",
        "archtechx/enums": "^0.3",
        "dragon-code/laravel-support": "^3.6",
        "dragon-code/support": "^6.11",
        "illuminate/console": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/support": "^10.0",
        "nesbot/carbon": "^2.68",
        "spatie/laravel-data": "^3.7",
        "symfony/console": "^6.3"
    },
    "require-dev": {
        "orchestra/testbench": "^8.5",
        "pestphp/pest": "^2.8",
        "symfony/var-dumper": "^6.3"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "CashierProvider\\Core\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dragon-code/codestyler": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.x-dev"
        },
        "laravel": {
            "providers": [
                "CashierProvider\\Core\\Providers\\ServiceProvider",
                "CashierProvider\\Core\\Providers\\ObserverServiceProvider",
                "CashierProvider\\Core\\Providers\\RateLimiterServiceProvider"
