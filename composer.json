{
    "name": "cashbox/core",
    "description": "Cashier provides an expressive, fluent interface to manage billing services.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "bank",
        "billing",
        "cashier",
        "payments"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@dragon-code.pro",
            "homepage": "https://github.com/andrey-helldar"
        }
    ],
    "support": {
        "issues": "https://github.com/cashbox/foundation/issues",
        "source": "https://github.com/cashbox/foundation"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/TheDragonCode"
        },
